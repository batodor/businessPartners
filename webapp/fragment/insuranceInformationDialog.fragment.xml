<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>insureInformation}" class="sapUiContentPadding" id="insuranceInformationDialog">
		<content>
			<Text text="{i18n>amountOfInsuredReceivables}" width="250px"/>
			<Input value="{Amount}" width="167px" class="margin-right"/>
			<Select selectedKey="{Currency}" items="{ path: '/DictionaryCurrencySet', sorter: { path: 'Currency' } }" width="80px">
				<core:Item key="{Currency}" text="{Currency}"/>
			</Select>
			<ToolbarSpacer/>
			<Text text="{i18n>insurancePremiumRatePercent}" width="250px"/>
			<Input value="{Rate}" width="167px" class="margin-right"/>
			<Input value="{Unit}" id="iInsuranceUnit" width="80px" editable="false"/>
			<ToolbarSpacer/>
			<Text text="{i18n>contactNumber}" width="250px"/>
			<Input value="{ContractNum}" width="255px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>validityDate}" width="250px"/>
			<DatePicker dateValue="{DateTo}" width="260px" class="sapUiSmallMarginBottom"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" type="Back" custom:id="insuranceInformationDialog" press="dialogClose"/>
			<Button text="{i18n>edit}" type="Accept" custom:id="insuranceInformation" custom:create="true" custom:url="/CounterpartyInformationSet" 
				custom:expandUrl="/ToRatingInsure" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>