<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>insuranceInformation}" class="sapUiContentPadding" id="insuranceInformationDialog">
		<content>
			<Label text="{i18n>amountOfInsurancedReceivables}" width="250px"/>
			<Input value="{Amount}" width="167px" class="margin-right"/>
			<Select selectedKey="{Currency}" items="{ path: '/DictionaryCurrencySet', sorter: { path: 'Currency' } }" width="80px">
				<core:Item key="{Currency}" text="{Currency}"/>
			</Select>
			<ToolbarSpacer/>
			<Label text="{i18n>insurancePremiumRatePercent}" width="250px"/>
			<Input
				value="{Rate}"
				type="Number"
				custom:max="3"
				custom:decimal="2"
				placeholder="0"
				change="checkDecimal"
				width="167px"
				class="margin-right"/>
            <!-- <MaskInput value="{Rate}" mask = "999.99" placeholder="Enter digital value" width="167px" class="margin-right"/> -->
			<Input value="%" id="iInsuranceUnit" width="80px" editable="false"/>
			<ToolbarSpacer/>
			<Label text="{i18n>contractNumber}" width="250px"/>
			<Input value="{ContractNum}" width="255px"/>
			<ToolbarSpacer/>
			<Label text="{i18n>validityDate}" width="250px"/>
			<DatePicker dateValue="{DateTo}" width="260px" class="sapUiSmallMarginBottom"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" type="Back" custom:id="insuranceInformationDialog" press="dialogClose"/>
			<Button text="{i18n>save}" type="Accept" custom:id="insuranceInformation" custom:create="true" custom:url="/RatingInsureSet" 
				custom:expandUrl="/ToRatingInsure" press="dialogEdit"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>