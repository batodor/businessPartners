<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" 
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{i18n>attachments}" class="sapUiContentPadding" id="uploadDashboardDialog">
		<content>
			<Text text="{i18n>type}" width="150px"/>
			<Select forceSelection="false" selectedKey="{Type}" items="{ path: '/DictionaryRatingSet', sorter: { path: 'Rating' } }" width="250px">
				<core:Item key="{Rating}" text="{Rating}"/>
			</Select>
			<ToolbarSpacer/>
			<Text text="{i18n>file}" width="150px"/>
			<u:FileUploader name="myFileUpload" uploadUrl="upload/" tooltip="Upload your file to the local server"
				uploadComplete="handleUploadComplete" change="handleValueChange" typeMissmatch="handleTypeMissmatch" style="Emphasized"
				fileType="txt,jpg,pdf,doc,docx">
				<u:parameters>
					<u:FileUploaderParameter name="param1" value="Foo"/>
					<u:FileUploaderParameter name="param2" value="bar"/>
				</u:parameters>
				<u:headerParameters>
					<u:FileUploaderParameter name="headerParam1" value="Foo"/>
					<u:FileUploaderParameter name="headerParam2" value="bar"/>
				</u:headerParameters>
			</u:FileUploader>
			<Button text="Upload File" press="handleUploadPress"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" type="Back" custom:id="uploadDashboardDialog" press="dialogClose"/>
			<Button text="{i18n>edit}" visible="false" custom:id="uploadDashboard" type="Accept" press="dialogEdit"/>
			<Button text="{i18n>add}" visible="false" custom:id="uploadDashboard" custom:url="/ComplianceRisksSet" type="Accept" press="dialogAdd"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>