<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<l:BlockLayout >
		<l:BlockLayoutRow>
			<l:BlockLayoutCell>
				<OverflowToolbar width="100%">
					<Title text="{i18n>management}" titleStyle="H3"/>
					<ToolbarSpacer/>
					<Button id="managementAdd" custom:id="management" text="{i18n>add}" press="tableAdd"/>
					<Button id="managementEdit" custom:id="management" text="{i18n>edit}" press="tableEdit" enabled="false"/>
					<Button id="managementDelete" custom:id="management" text="{i18n>delete}" press="tableDelete" type="Reject" enabled="false"/>
				</OverflowToolbar>
				<Table id="managementTable" custom:id="management" width="100%" items="{}" mode="SingleSelectLeft" includeItemInSelection="true" selectionChange="onTableSelect"
					noDataText="{worklistView>/tableNoDataText}" busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingScrollToLoad="true">
					<columns>
						<Column><Label class="black" text="{i18n>name}"/></Column>
						<Column><Label class="black" text="{i18n>surname}"/></Column>
						<Column><Label class="black" text="{i18n>position}"/></Column>
						<Column><Label class="black" text="{i18n>typeOfGovernance}"/></Column>
						<Column><Label class="black" text="{i18n>contactType}"/></Column>
						<Column><Label class="black" text="{i18n>phone}"/></Column>
						<Column><Label class="black" text="{i18n>fax}"/></Column>
						<Column><Label class="black" text="{i18n>email}"/></Column>
					</columns>
					<items>
						<ColumnListItem>
							<cells>
								<Text text="{Name}"/>
								<Text text="{Surname}"/>
								<Text text="{Position}"/>
								<Text custom:key="{GoverType}" text="{GoverName}"/>
								<Text custom:key="{ContactType}" text="{ContactName}"/>
								<Text text="{Phone}"/>
								<Text text="{FaxNumber}"/>
								<Text text="{Email}"/>
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</l:BlockLayoutCell>
		</l:BlockLayoutRow>
	</l:BlockLayout>
	<l:BlockLayout>
		<l:BlockLayoutRow>
			<l:BlockLayoutCell>
				<OverflowToolbar width="100%">
					<Title text="{i18n>proxy}" titleStyle="H3"/>
					<ToolbarSpacer/>
					<Button id="proxyAdd" custom:id="proxy" custom:upload="uploadProxy" text="{i18n>add}" press="tableAdd"/>
					<Button id="proxyEdit" custom:id="proxy" custom:upload="uploadProxy" text="{i18n>edit}" press="tableEdit" enabled="false"/>
					<Button id="proxyDelete" custom:id="proxy" text="{i18n>delete}" press="tableDelete" type="Reject" enabled="false"/>
				</OverflowToolbar>
				<Table id="proxyTable" width="100%" custom:id="proxy" items="{}" mode="SingleSelectLeft" includeItemInSelection="true" selectionChange="onTableSelect"
					noDataText="{worklistView>/tableNoDataText}" busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingScrollToLoad="true">
					<columns>
						<Column><Label class="black" text="{i18n>name}"/></Column>
						<Column><Label class="black" text="{i18n>surname}"/></Column>
						<Column><Label class="black" text="{i18n>position}"/></Column>
						<Column><Label class="black" text="{i18n>comment}"/></Column>
						<Column><Label class="black" text="{i18n>contactType}"/></Column>
						<Column><Label class="black" text="{i18n>phone}"/></Column>
						<Column><Label class="black" text="{i18n>fax}"/></Column>
						<Column><Label class="black" text="{i18n>email}"/></Column>
						<Column><Label class="black" text="{i18n>file}"/></Column>
					</columns>
					<items>
						<ColumnListItem>
							<cells>
								<Text text="{Name}"/>
								<Text text="{Surname}"/>
								<Text text="{Position}"/>
								<Text text="{Comment}"/>
								<Text custom:key="{ContactType}" text="{ContactName}"/>
								<Text text="{Phone}"/>
								<Text text="{FaxNumber}"/>
								<Text text="{Email}"/>
								<List items="{ path: 'ToAttachments', templateShareable:false }" class="attachmentsList" showNoData="false">
									<CustomListItem>
										<Button icon="sap-icon://download" press="downloadButton" />
									</CustomListItem>
								</List>
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</l:BlockLayoutCell>
		</l:BlockLayoutRow>
	</l:BlockLayout>
</core:FragmentDefinition>