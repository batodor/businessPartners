<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="Edit Management" class="sapUiContentPadding">
		<content>
			<Text text="{i18n>name}" width="170px"/>
			<Input id="iManagementName" width="300px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>surname}" width="170px"/>
			<Input id="iManagementSurname" width="300px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>position}" width="170px"/>
			<Input id="iManagementPosition" width="300px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>typeOfGovernance}" width="170px"/>
			<Select id="sManagementTypeOfGov" width="300px" items="{ path: '/DictionaryGoverTypeSet', sorter: { path: 'GoverType' } }">
				<core:Item key="{GoverType}" text="{GoverType} - {GoverTypeName}"/>
			</Select>
			<ToolbarSpacer/>
			<Text text="{i18n>phone}" width="170px"/>
			<Input id="iManagementPhone" width="300px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>fax}" width="170px"/>
			<Input id="iManagementFax" width="300px"/>
			<ToolbarSpacer/>
			<Text text="{i18n>email}" width="170px"/>
			<Input id="iManagementEmail" width="300px"/>
		</content>
		<buttons>
			<Button text="{i18n>cancel}" type="Back" custom:dialog="dManagement" press="closeDialog"/>
			<Button text="{i18n>edit}" visible="false" type="Accept" press="dialogEditManagement"/>
			<Button text="{i18n>add}" visible="false" type="Accept" press="dialogAddManagement"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>