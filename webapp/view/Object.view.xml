<mvc:View controllerName="counterparties.Counterparties.controller.Object" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc"
	xmlns:semantic="sap.m.semantic" xmlns:u="sap.ui.unified" xmlns:core="sap.ui.core" xmlns:footerbar="sap.ushell.ui.footerbar"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<semantic:FullscreenPage id="page" title="{i18n>objectViewTitle}" busy="{objectView>/busy}" busyIndicatorDelay="{objectView>/delay}">
		<l:BlockLayout id="blMainInf">
			<l:BlockLayoutRow>
				<l:BlockLayoutCell width="50%">
					<Title id="tCompanyName" text="{Name}" titleStyle="H1"/>
					<ToolbarSpacer/>
					<Label id="tSAPID" text="{Code}"/>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell width="25%">
					<Label class="black" text="{i18n>type}:" width="100px"/>
					<Label text="{Type}"/>
					<ToolbarSpacer/>
					<Label class="black" text="{i18n>legalEntity}:" width="100px"/>
					<Label text="{Company}"/>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell width="25%">
					<Label class="black" text="{i18n>country}:" width="150px"/>
					<Label text="{Country}"/>
					<ToolbarSpacer/>
					<Label class="black" text="{i18n>regDate}:" width="150px"/>
					<Label text="{ path: 'DateCreation', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>
		</l:BlockLayout>
		<IconTabBar id="itbMain" select="onTabSelected" selectedKey="dashboard">
			<items>
				<IconTabFilter text="{i18n>dashboard}" key="dashboard">
					<l:BlockLayout id="blGenInf">
						<l:BlockLayoutRow>
							<l:BlockLayoutCell id="cGenInf">
								<OverflowToolbar>
									<Title text="{i18n>genInf}" titleStyle="H3"/>
								</OverflowToolbar>
								<Label text=""/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>codeFromKioda}:" width="170px"/>
								<Label text="{Identifier}" id="lMainInfIdentifier" />
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>regNumber}:" width="170px"/>
								<Label text="{RegistrationNumber}" id="lMainInfRegistrationNumber"/>
								<ToolbarSpacer/>
								<FlexBox width="100%" class="columns" alignItems="Stretch">
									<items>
										<Label class="black" text="{i18n>legalForm}:" width="170px"/>
										<Label text="{LegalForm}" id="lMainInfLegalForm" />
										<Input class="width-auto" id="iMainInfLegalForm" value="{LegalForm}" width="180px" visible="false" />
									</items>
								</FlexBox>
								<ToolbarSpacer/>
								<FlexBox width="100%" class="columns" alignItems="Stretch">
									<items>
										<Label class="black" text="{i18n>legalName}:" width="170px"/>
										<Label text="{LegalName}" id="lMainInfLegalName"/>
									</items>
								</FlexBox>
								<ToolbarSpacer/>
								<FlexBox width="100%" class="columns" alignItems="Stretch">
									<items>
										<Label class="black" text="{i18n>englishName}:" width="170px"/>
										<Label text="{EnglishName}" id="lMainInfEnglishName"/>
									</items>
								</FlexBox>
							</l:BlockLayoutCell>
							<l:BlockLayoutCell>
								<OverflowToolbar>
									<Title text="{i18n>addInf}" titleStyle="H3"/>
								</OverflowToolbar>
								<Label text=""/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>relCat}:" width="191px"/>
								<Label text="{RelationCategory}" id="lMainInfRelationCategory"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>relBP}:" width="191px"/>
								<Label text="{RelationBP}" id="lMainInfRelationBP"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>limitSecurity}:" width="191px"/>
								<Label
									text="{ parts: [ {path: 'LimitSecurity'}, {path: 'Currency'} ], type: 'sap.ui.model.type.Currency', formatOptions: { showMeasure: false } }"
									class="margin-right"
									id="lMainInfLimitSecurity" />
								<Input value="{LimitSecurity}" id="iMainInfLimitSecurity" class="margin-right" width="165px" visible="false" />
								<Label text="{Currency}" id="lMainInfCurrency" />
								<Select id="sMainInfCurrency" selectedKey="{Currency}" items="{ path: '/DictionaryCurrencySet', sorter: { path: 'Currency' } }" width="80px" visible="false">
									<core:Item key="{Currency}" text="{Currency}"/>
								</Select>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>validityDate}:" width="191px"/>
								<Label id="lMainInfDateValidity" text="{ path: 'DateValidity', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>
								<DatePicker id="dpMainInfDateValidity" placeholder="{i18n>enterDate}" value="{DateValidity}" width="170px" visible="false" />
								<ToolbarSpacer/>
								<Label class="black" id="lDownloadDoc" text="{i18n>downloadDocument}:" width="191px"/>
								<Link id="linkDownloadDoc" text="document.pdf" press="handleLinkPress"/>
								<ToolbarSpacer/>
								<u:FileUploader enabled="false" id="fuMainInfFileUpload" name="myFileUpload" uploadUrl="upload/" tooltip="Upload your file to the local server"
									uploadComplete="handleUploadComplete" change="handleValueChange" typeMissmatch="handleTypeMissmatch" style="Emphasized"
									fileType="txt,jpg,pdf,doc,docx" placeholder="{i18n>chooseFile}">
									<u:parameters>
										<u:FileUploaderParameter name="param1" value="Foo"/>
										<u:FileUploaderParameter name="param2" value="bar"/>
									</u:parameters>
									<u:headerParameters>
										<u:FileUploaderParameter name="headerParam1" value="Foo"/>
										<u:FileUploaderParameter name="headerParam2" value="bar"/>
									</u:headerParameters>
								</u:FileUploader>
								<Button enabled="false" id="bMainInfFileUpload" text="Upload File" press="handleUploadPress"/>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
					<l:BlockLayout>
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar>
									<Title text="{i18n>addressBook}" titleStyle="H3"/>
								</OverflowToolbar>
								<Table id="addressTable" width="auto" items="{}" noDataText="{worklistView>/tableNoDataText}"
									busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingThreshold="5" growingScrollToLoad="false">
									<columns>
										<Column width="100px"><Label class="black" text="{i18n>addressType}"/></Column>
										<Column width="100px"><Label class="black" text="{i18n>country}"/></Column>
										<Column width="120px"><Label class="black" text="{i18n>region}"/></Column>
										<Column width="90px"><Label class="black" text="{i18n>ZIP}"/></Column>
										<Column width="120px"><Label class="black" text="{i18n>city}"/></Column>
										<Column width="200px"><Label class="black" text="{i18n>street}"/></Column>
										<Column width="100px"><Label class="black" text="{i18n>building}"/></Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<Text text="{AddressType}"/>
												<Text text="{Country}"/>
												<Text text="{Region}"/>
												<Text text="{ZIP}"/>
												<Text text="{City}"/>
												<Text text="{Street}"/>
												<Text text="{Office}"/>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
					<l:BlockLayout>
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar>
									<Title text="{i18n>bankAccount}" titleStyle="H3"/>
								</OverflowToolbar>
								<Table id="bankAccountTable" width="auto" items="{}" noDataText="{worklistView>/tableNoDataText}"
									busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingThreshold="5" growingScrollToLoad="false">
									<columns>
										<Column width="100px"><Label class="black" text="{i18n>country}"/></Column>
										<Column width="180px"><Label class="black" text="{i18n>bank}"/></Column>
										<Column width="90px"><Label class="black" text="{i18n>swift}"/></Column>
										<Column><Label class="black" text="{i18n>address}"/></Column>
										<Column width="120px"><Label class="black" text="{i18n>bcAcc}"/></Column>
										<Column width="120px"><Label class="black" text="{i18n>iban}"/></Column>
										<Column><Label class="black" text="{i18n>accountName}"/></Column>
										<Column><Label class="black" text="{i18n>account}"/></Column>
										<Column width="60px"><Label class="black" text="{i18n>currency}"/></Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<Text text="{Country}"/>
												<Text text="{BankName}"/>
												<Text text="{SWIFT}"/>
												<Text text="{Address}"/>
												<Text text="{AccountBC}"/>
												<Text text="{IBAN}"/>
												<Text text="{AccountName}"/>
												<Text text="{Account}"/>
												<Text text="{Currency}"/>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
				</IconTabFilter>
				<IconTabFilter text="{i18n>government}" key="government">
					<l:BlockLayout >
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar width="100%">
									<Title text="{i18n>management}" titleStyle="H3"/>
									<ToolbarSpacer/>
									<Button id="bAddManagement" text="{i18n>add}" press="tableAddManagement"/>
									<Button id="bEditManagement" text="{i18n>edit}" press="tableEditManagement" enabled="false"/>
									<Button id="bDeleteManagement" text="{i18n>delete}" press="tableDeleteManagement" type="Reject" enabled="false"/>
								</OverflowToolbar>
								<Table id="managementTable" width="100%" items="{}" mode="SingleSelectLeft" selectionChange="onManagementChange"
									noDataText="{worklistView>/tableNoDataText}" busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingScrollToLoad="true">
									<columns>
										<Column><Label class="black" text="{i18n>name}"/></Column>
										<Column><Label class="black" text="{i18n>surname}"/></Column>
										<Column><Label class="black" text="{i18n>position}"/></Column>
										<Column><Label class="black" text="{i18n>typeOfGovernance}"/></Column>
										<Column><Label class="black" text="{i18n>phone}"/></Column>
										<Column><Label class="black" text="{i18n>fax}"/></Column>
										<Column><Label class="black" text="{i18n>email}"/></Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<Text text="{Name}"/>
												<Text text="{Surname}"/>
												<Text text="{Position}"/>
												<Text custom:key="{GoverType}" text="{GoverName}"/>
												<Text text="{Phone}"/>
												<Text text="{FaxNumber}"/>
												<Text text="{Email}"/>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
					<l:BlockLayout>
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar width="100%">
									<Title text="{i18n>proxy}" titleStyle="H3"/>
									<ToolbarSpacer/>
									<Button id="bAddProxy" text="{i18n>add}" press="tableAddProxy"/>
									<Button id="bEditProxy" text="{i18n>edit}" press="tableEditProxy" enabled="false"/>
									<Button id="bDeleteProxy" text="{i18n>delete}" press="tableDeleteProxy" type="Reject" enabled="false"/>
								</OverflowToolbar>
								<Table id="proxyTable" width="100%" items="{}" mode="SingleSelectLeft" selectionChange="onProxyChange"
									noDataText="{worklistView>/tableNoDataText}" busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingScrollToLoad="true">
									<columns>
										<Column><Label class="black" text="{i18n>name}"/></Column>
										<Column><Label class="black" text="{i18n>surname}"/></Column>
										<Column><Label class="black" text="{i18n>position}"/></Column>
										<Column><Label class="black" text="{i18n>comment}"/></Column>
										<Column><Label class="black" text="{i18n>phone}"/></Column>
										<Column><Label class="black" text="{i18n>fax}"/></Column>
										<Column><Label class="black" text="{i18n>email}"/></Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<Text text="{Name}"/>
												<Text text="{Surname}"/>
												<Text text="{Position}"/>
												<Text text="{Comment}"/>
												<Text text="{Phone}"/>
												<Text text="{FaxNumber}"/>
												<Text text="{Email}"/>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
				</IconTabFilter>
				<IconTabFilter text="{i18n>rating}" key="rating">
					<l:BlockLayout id="blRatingGenInf">
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar>
									<Title text="{i18n>rating}" titleStyle="H3"/>
								</OverflowToolbar>
								<Label text=""/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>countryRating}:" width="240px"/>
								<Label text="{CountryRating}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>rBusinessScale}:" width="240px"/>
								<Label id="lBusinessScale" text="{BusinessScale}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>rCorporateTransparency}:" width="240px"/>
								<Label id="lCorporateTransparency" text="{CorporateTransparency}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>rFinancialProfile}:" width="240px"/>
								<Label id="lFinancialProfile" text="{FinancialProfile}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>rCommercialProfile}:" width="240px"/>
								<Label id="lCommercialProfile" text="{CommercialProfile}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>rSet}:" width="240px"/>
								<Label id="lRatingSet" text="{RatingSet}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>expressScore}:" width="240px"/>
								<CheckBox class="rowCheckbox" id="chExpressScore" selected="{ExpressScore}" editable="false"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>date}:" width="240px"/>
								<Label id="lRatingDate" custom:key="{Date}"
									text="{ path: 'Date', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>
								<ToolbarSpacer/>
								<Button text="{i18n>updateRating}" type="Emphasized" press="showRating"/>
							</l:BlockLayoutCell>
							<l:BlockLayoutCell>
								<OverflowToolbar>
									<Title text="{i18n>limitsCurrentRating}" titleStyle="H3"/>
								</OverflowToolbar>
								<Label text=""/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>tonnage}:" width="240px"/>
								<Label text=""/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>period}:" width="240px"/>
								<Label text=""/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>paymentTerms}:" width="240px"/>
								<Label text=""/>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
					<l:BlockLayout>
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar>
									<Title text="{i18n>historicalData}" titleStyle="H3"/>
								</OverflowToolbar>
								<Table id="historicalDataTable" width="auto" items="{}" noDataText="{worklistView>/tableNoDataText}"
									busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingThreshold="5" growingScrollToLoad="false">
									<columns>
										<Column hAlign="Center" width="100px"><Label class="black" text="{i18n>expressScore}"/></Column>
										<Column hAlign="Center" width="100px"><Label class="black" text="{i18n>date}"/></Column>
										<Column hAlign="Center" width="100px"><Label class="black" text="{i18n>businessScale}"/></Column>
										<Column hAlign="Center" width="100px"><Label class="black" text="{i18n>financeProfile}"/></Column>
										<Column hAlign="Center" width="100px"><Label class="black" text="{i18n>commercialProfile}"/></Column>
										<Column hAlign="Center" width="100px"><Label class="black" text="{i18n>corporateTransparency}"/></Column>
										<Column hAlign="Center" width="100px"><Label class="black" text="{i18n>rSet}"/></Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<CheckBox selected="{ExpressScore}" editable="false"/>
												<Text text="{ path: 'Date', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>
												<Text text="{BusinessScale}"/>
												<Text text="{FinancialProfile}"/>
												<Text text="{CommercialProfile}"/>
												<Text text="{CorporateTransparency}"/>
												<Text text="{RatingSet}"/>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
					<l:BlockLayout id="blCreditLimit">
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar width="500px">
									<Title text="{i18n>validatedCreditLimit}" titleStyle="H3"/>
								</OverflowToolbar>
								<Label text=""/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>creditLimit}:" width="240px"/>
								<Label id="lCreditLimit" custom:creditLimit="{CreditLimit}"
									text="{ parts: [ {path: 'CreditLimit'}, {path: 'Currency'} ], type: 'sap.ui.model.type.Currency', formatOptions: { showMeasure: false } }"
									class="margin-right"/>
								<Label id="lCreditLimitCurrency" text="{Currency}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>validityDate}:" width="240px"/>
								<Label id="lCreditLimitValidityDate" custom:validityDate="{ValidityDate}"
									text="{ path: 'ValidityDate', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>
								<ToolbarSpacer/>
								<Button text="{i18n>updateCreditLimit}" type="Emphasized" press="showCreditLimit"/>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
					<l:BlockLayout>
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar width="500px">
									<Title text="{i18n>historicalData}" titleStyle="H3"/>
								</OverflowToolbar>
								<Table id="historicalDataTable2" width="500px" items="{}" noDataText="{worklistView>/tableNoDataText}"
									busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingThreshold="5" growingScrollToLoad="false">
									<columns>
										<Column hAlign="Center" width="80px"><Label class="black" text="{i18n>creditLimit}"/></Column>
										<Column hAlign="Center" width="80px"><Label class="black" text="{i18n>currency}"/></Column>
										<Column hAlign="Center" width="80px"><Label class="black" text="{i18n>validityDate}"/></Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<Text
													text="{ parts: [ {path: 'CreditLimit'}, {path: 'Currency'} ], type: 'sap.ui.model.type.Currency', formatOptions: { showMeasure: false } }"/>
												<Text text="{Currency}"/>
												<Text text="{path: 'ValidityDate', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
					<l:BlockLayout id="blInsuranceInf">
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar width="500px">
									<Title text="{i18n>insuranceInformation}" titleStyle="H3"/>
								</OverflowToolbar>
								<Label text=""/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>amountOfInsurancedReceivables}:" width="280px"/>
								<Label id="lInsuranceReceivables" custom:receivables="{Amount}"
									text="{ parts: [ {path: 'Amount'}, {path: 'Currency'} ], type: 'sap.ui.model.type.Currency', formatOptions: { showMeasure: false } }"
									class="margin-right"/>
								<Label id="lInsuranceCurrency" text="{Currency}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>insurancePremiumRatePercent}:" width="280px"/>
								<Label id="lInsuranceRate" custom:rate="{Rate}" custom:unit="{Unit}" text="{Rate} {Unit}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>contractNumber}:" width="280px"/>
								<Label id="lInsuranceContract" text="{ContractNum}"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>validityDate}:" width="280px"/>
								<Label id="lInsuraceValidityDate" custom:validityDate="{DateTo}"
									text="{path: 'DateTo', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>
								<ToolbarSpacer/>
								<Button text="{i18n>updateInsuranceInformation}" type="Emphasized" press="showInsuranceInf"/>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
				</IconTabFilter>
				<IconTabFilter text="{i18n>complianceRisks}" key="risks">
					<l:BlockLayout>
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<Title text="{i18n>complianceRisks}" titleStyle="H3"/>
								<ToolbarSpacer/>
								<Label text="{i18n>countryUnderSanction}" class="red" id="isUnderSanction"/>
								<OverflowToolbar>
									<Label class="black" text="{i18n>country}:" width="80px"/>
									<Label text="{Country}"/>
									<ToolbarSpacer/>
									<Button id="bAddRisk" text="{i18n>add}" press="tableAddRisk"/>
									<Button id="bEditRisk" text="{i18n>edit}" press="tableEditRisk" enabled="false"/>
									<Button id="bDeleteRisk" text="{i18n>delete}" press="tableDeleteRisk" type="Reject" enabled="false"/>
								</OverflowToolbar>
								<Table id="complianceRisksTable" mode="SingleSelectLeft" selectionChange="onRiskSelect" width="auto" items="{}"
									noDataText="{worklistView>/tableNoDataText}" busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingScrollToLoad="true">
									<columns>
										<Column width="120px"><Label class="black" text="{i18n>type}"/></Column>
										<!--<Column width="100px"><Label class="black" text="{i18n>itemNumber}"/></Column>-->
										<Column><Label class="black" text="{i18n>description}"/></Column>
										<Column><Label class="black" text="{i18n>recommendedActions}"/></Column>
										<Column width="80px"><Label class="black" text="{i18n>dateFrom}"/></Column>
										<Column width="80px"><Label class="black" text="{i18n>dateTo}"/></Column>
										<Column width="80px"><Label class="black" text="{i18n>actual}"/></Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<Text custom:key="{RiskType}" text="{Type}"/>
												<!--<Text text="{ItemNumber}"/>-->
												<Text text="{Description}"/>
												<Text text="{Actions}"/>
												<Text custom:key="{DateFrom}"
													text="{path: 'DateFrom', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' }, constraints:{displayFormat:'Date'} }"/>
												<Text custom:key="{DateTo}"
													text="{path: 'DateTo', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' }, constraints:{displayFormat:'Date'} }"/>
												<CheckBox selected="{Actual}" editable="false"/>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
					<l:BlockLayout>
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar width="100%">
									<Title text="{i18n>politicalExposedPerson}" titleStyle="H3"/>
									<ToolbarSpacer/>
									<Button text="{i18n>add}" press="tableAddPolitical"/>
									<Button text="{i18n>edit}" press="tableEditPolitical" id="bEditPolitical" enabled="false"/>
									<Button text="{i18n>delete}" press="tableDeletePolitical" id="bDeletePolitical" type="Reject" enabled="false"/>
								</OverflowToolbar>
								<Table id="politicalExposedTable" width="100%" mode="SingleSelectLeft" selectionChange="onPersonChange" items="{}"
									noDataText="{worklistView>/tableNoDataText}" busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingScrollToLoad="true">
									<columns>
										<Column><Label class="black" text="{i18n>name}"/></Column>
										<Column><Label class="black" text="{i18n>surname}"/></Column>
										<Column><Label class="black" text="{i18n>position}"/></Column>
										<Column><Label class="black" text="{i18n>comment}"/></Column>
										<Column><Label class="black" text="{i18n>phone}"/></Column>
										<Column><Label class="black" text="{i18n>fax}"/></Column>
										<Column><Label class="black" text="{i18n>email}"/></Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<Text text="{Name}"/>
												<Text text="{Surname}"/>
												<Text text="{Position}"/>
												<Text text="{Comment}"/>
												<Text text="{Phone}"/>
												<Text text="{FaxNumber}"/>
												<Text text="{Email}"/>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
					<l:BlockLayout>
						<l:BlockLayoutRow>
							<l:BlockLayoutCell id="blcBlacklisted">
								<OverflowToolbar width="600px">
									<Title text="{i18n>blacklistedInf}" titleStyle="H3"/>
								</OverflowToolbar>
								<Label text=""/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>blacklisted}:" width="140px"/>
								<CheckBox id="cbBlacklist" selected="{BlackListed}" class="rowCheckbox" editable="false"/>
								<ToolbarSpacer/>
								<Label class="black" text="{i18n>justification}:" width="140px"/>
								<TextArea id="taBlacklistJustification" value="{Justification}" width="460px" height="80px" enabled="false" />
								<ToolbarSpacer/>
								<Button text="{i18n>updateBlacklistInf}" type="Emphasized" press="showBlacklist"/>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
						<l:BlockLayoutRow>
							<l:BlockLayoutCell>
								<OverflowToolbar width="600px">
									<Title text="{i18n>historicalData}" titleStyle="H3"/>
								</OverflowToolbar>
								<Table id="blacklistedInfTable" width="600px" items="{}" noDataText="{worklistView>/tableNoDataText}"
									busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingThreshold="5" growingScrollToLoad="false">
									<columns>
										<Column width="70px"><Label class="black" text="{i18n>blacklisted}"/></Column>
										<Column><Label class="black" text="{i18n>justification}"/></Column>
										<Column width="100px"><Label class="black" text="{i18n>user}"/></Column>
										<Column width="80px"><Label class="black" text="{i18n>date}"/></Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<CheckBox selected="{BlackListed}" editable="false"/>
												<Text class="black" text="{Justification}"/>
												<Text text="{User}"/>
												<Text text="{path: 'DateFrom', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' } }"/>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
				</IconTabFilter>
				<!--<IconTabFilter text="{i18n>deals}" key="deals">-->
				<!--	<Label text="People content goes here ..."/>-->
				<!--</IconTabFilter>-->
				<IconTabFilter text="{i18n>attachments}" key="attachments">
					<UploadCollection change="changeAttachment" id="attachmentsTable" multiple="true" uploadComplete="onUploadComplete" uploadEnabled="true">
						<UploadCollectionItem contributor="{OrderID}" enableEdit="false" fileName="{OrderID}" fileSize="20mb" mimeType="File Type"
							thumbnailUrl="{thumbnailUrl}"
							uploadedDate="{ path: 'RequiredDate', type: 'sap.ui.model.type.Date', formatOptions: { style: 'short', pattern: 'dd.MM.yyyy' } }"/>
					</UploadCollection>
				</IconTabFilter>
			</items>
		</IconTabBar>
		<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>
		</semantic:sendEmailAction>
		<semantic:shareInJamAction>
			<semantic:ShareInJamAction id="shareInJam" visible="{FLP>/isShareInJamActive}" press="onShareInJamPress"/>
		</semantic:shareInJamAction>
		<semantic:saveAsTileAction>
			<footerbar:AddBookmarkButton id="shareTile" title="{objectView>/saveAsTileTitle}"/>
		</semantic:saveAsTileAction>
		<semantic:editAction>
			<semantic:EditAction id="editMainInf" title="{i18n>edit}" press="editMainInf"/>
		</semantic:editAction>
		<semantic:saveAction>
			<semantic:SaveAction id="saveMainInf" title="{i18n>save}" visible="false" press="saveMainInf"/>
		</semantic:saveAction>
		<semantic:cancelAction>
			<semantic:CancelAction id="cancelMainInf" title="{i18n>cancel}" visible="false" press="cancelMainInf"/>
		</semantic:cancelAction>
	</semantic:FullscreenPage>
</mvc:View>